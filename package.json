{
  "name": "finriq",
  "version": "2.0.0",
  "description": "Bot designed for the Black Cat Inn.",
  "main": "node bot.js",
  "engines": {
    "node": ">=18"
  },
  "prisma": {
    "schema": "database/schema.prisma"
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "start": "tsc && node dist/bot.js",
    "watch": "tsc-watch --noClear -p tsconfig.json --onSuccess 'node dist/bot.js'",
    "prettier": "prettier --write package.json tsconfig.json src/**/*.ts"
  },
  "author": {
    "name": "Benjamin Hollon",
    "url": "https://seewitheyesclosed.com/"
  },
  "contributors": [
    {
      "name": "Zachary Montgomery",
      "url": "https://codingprojects.org/"
    }
  ],
  "dependencies": {
    "@prisma/client": "^3.13.0",
    "cron": "^1.8.2",
    "data-store": "^4.0.3",
    "discord.js": "^13.1.0",
    "eslint-plugin-jest": "^25.2.4",
    "fastest-levenshtein": "^1.0.12",
    "moment": "^2.29.4",
    "mongodb": "^4.2.0",
    "mysql2": "^2.0.2",
    "prettier": "^2.4.1",
    "prisma": "^3.13.0",
    "rss-parser": "^3.7.2",
    "sequelize": "^6.28.2",
    "set-value": "^4.0.1"
  },
  "devDependencies": {
    "eslint": "^8.2.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "tsc": "^2.0.3",
    "tsc-watch": "^4.5.0",
    "typescript": "^4.4.3"
  }
}
